<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="nativescript-persona" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout class="p-20">
        <ScrollView class="h-full">
          <StackLayout>
            <Button text="Start FromTemplate" tap="{{ startFromTemplate }}" class="btn btn-primary"/>
            <Button text="Start FromInquiry" tap="{{ startFromInquiry }}" class="btn btn-primary"/>
          </StackLayout>
        </ScrollView>
    </StackLayout>
</Page>