    "all": {
      "root": "packages/",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages"
          ],
          "options": {
            "commands": [
              "nx run nativescript-filterable-listpicker:build.all",
              "nx run nativescript-exoplayer:build.all",
              "nx run nativescript-checkbox:build.all",
              "nx run nativescript-input-mask:build.all",
              "nx run nativescript-onfido:build.all",
              "nx run nativescript-plaid:build.all",
              "nx run nativescript-camera-plus:build.all",
              "nx run nativescript-dynatrace:build.all",
              "nx run nativescript-tracking-transparency:build.all",
              "nx run nativescript-fancyalert:build.all"
            ],
            "parallel": false
          }
        },
        "focus": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [
            "dist/packages"
          ],
          "options": {
            "commands": [
              "nx g @nativescript/plugin-tools:focus-packages"
            ],
            "parallel": false
          }
        }
      }
    },
